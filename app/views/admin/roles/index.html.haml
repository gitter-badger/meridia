.container
  .col-md-12
    .panel
      = link_to "Crear Rol", new_admin_role_path  
      %table.table
        %thead
          %tr
            %th
              Nombre
            %th
              Descripcion
        %tbody
          -@roles.each do |f|
            %tr
              %td
                = link_to  f.name, admin_role_path(f)
                
              %td
                = f.descripcion
