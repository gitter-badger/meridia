  .panel
    .well
      =form_for :search, url:  prospect_grafic_path ,method: :get ,html: {class: "form-inline"} do |f|
        %label Desde:
        = f.date_field :start_date,value: @date_start,class: "form-control"
        %label Hasta :
        = f.date_field :date_end,value: @date_end,class: "form-control"
        = f.submit  "Generar Informe", class: "btn btn-success"

    .row
      .col-md-6
        #container
      .col-md-6
        %h4
        #contact
    %hr      
    .row
      .col-md-6
        #level
      .col-md-6
        #tracing
= render "charts/prospect_grafic"

